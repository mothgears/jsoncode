{
  "name": "jsoncode",
  "version": "1.7.5",
  "description": "JSON with conditional statements",
  "author": "Artem Varshavin",
  "license": "MIT",
  "keywords": [
    "json",
    "conditional statements",
    "logical tree",
    "switch",
    "if",
    "spread"
  ],
  "main": "jsoncode.lib.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development node --experimental-modules --experimental-json-modules node_modules/jest/bin/jest.js",
    "build": "cross-env NODE_ENV=production babel jsoncode.mjs --plugins=transform-object-rest-spread,array-includes,starts-with-ends-with,add-module-exports --presets env --out-file jsoncode.lib.js",
    "build-test": "cross-env NODE_ENV=production jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mothgears/jsoncode.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-remove-import-export": "^1.1.0",
    "babel-plugin-starts-with-ends-with": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "cross-env": "^5.2.0",
    "jest": "^24.9.0",
    "jest-esm-transformer": "^1.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json"
    ],
    "testMatch": [
      "**/tests/**/*.(js|mjs)"
    ],
    "transform": {
      ".mjs": "jest-esm-transformer"
    }
  },
  "dependencies": {}
}
